<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.wolfii.ListAllSongsFragment"
        android:label="@string/queue"
        tools:layout="@layout/fragment_listallsongs" />

    <fragment
        android:id="@+id/navigation_artiste"
        android:name="com.example.wolfii.ListAllArtistsFragment"
        android:label="@string/queue_artist"
        tools:layout="@layout/fragment_listallsongs" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.wolfii.ControleMusiqueFragment"
        android:label="@string/lecteur"
        tools:layout="@layout/fragment_controle_musique" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.wolfii.ui.notifications.NotificationsFragment"
        tools:layout="@layout/fragment_notifications" />
</navigation>